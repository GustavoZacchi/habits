\n  align-items: flex-end;\n  background-size: contain;\n  background-repeat: no-repeat;\n  background-position: center;\n\n  span {\n    font-size: 1.3rem;\n  }\n`;\n\nexport const ContainerDesk = styled.div`\n  display: flex;\n  margin-left: auto;\n  flex-direction: row;\n  width: 70vw;\n  padding: 4px;\n  column-gap: 6px;\n`;\n","/home/gustazacchi/gestao-de-habitos/src/pages/Home/index.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/SignUp/index.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Groups/index.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Habits/index.js",["284"],"import { Modal, Backdrop } from \"@material-ui/core\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport Container, {\n  Button,\n  AnimationContainer,\n  BackgroundHeading,\n} from \"./styles\";\n\nimport { useState } from \"react\";\nimport CardList from \"../../components/Habits/CardList\";\nimport NewHabit from \"../../components/Habits/NewHabit\";\nimport { useContext } from \"react\";\nimport { HabitsContext } from \"../../providers/habits\";\nimport { Redirect } from \"react-router-dom\";\n\nconst Habits = () => {\n  const [open, setOpen] = useState(false);\n  const { token } = useContext(HabitsContext);\n\n  const useStyles = makeStyles((theme) => ({\n    modal: {\n      display: \"flex\",\n      alignItems: \"center\",\n      justifyContent: \"center\",\n      maxWidth: \"500px\",\n      margin: \"0 auto\",\n    },\n  }));\n  const classes = useStyles();\n\n  const handleOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  if (!token) {\n    return <Redirect to=\"/login\" />;\n  }\n\n  return (\n    <>\n      <Container>\n        <AnimationContainer>\n          <Button onClick={handleOpen}>New habit</Button>\n\n          <Modal\n            aria-labelledby=\"transition-modal-title\"\n            aria-describedby=\"transition-modal-description\"\n            className={classes.modal}\n            open={open}\n            onClose={handleClose}\n            closeAfterTransition\n            BackdropComponent={Backdrop}\n            BackdropProps={{\n              timeout: 500,\n            }}\n          >\n            <NewHabit handleClose={handleClose} opne={open} />\n          </Modal>\n        </AnimationContainer>\n      </Container>\n      <CardList />\n    </>\n  );\n};\n\nexport default Habits;\n","/home/gustazacchi/gestao-de-habitos/src/pages/Login/index.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/NotFound/index.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/MyAcount/index.js",["285","286","287","288","289"],"/home/gustazacchi/gestao-de-habitos/src/pages/Discovery/index.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Home/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/SignUp/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Habits/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Groups/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Login/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/NotFound/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/MyAcount/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/pages/Discovery/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/Habits/CardList/index.js",["290"],"import { useContext, useEffect } from \"react\";\nimport { HabitsContext } from \"../../../providers/habits\";\nimport \"react-responsive-carousel/lib/styles/carousel.min.css\";\nimport { Container } from \"./styles\";\nimport \"react-circular-progressbar/dist/styles.css\";\nimport Card from \"../Card\";\nimport CardDefault from \"../CardDefault\";\n\nconst CardList = () => {\n  const { habits, getHabits } = useContext(HabitsContext);\n\n  useEffect(() => {\n    getHabits();\n  }, []);\n\n  return (\n    <Container showThumbs={false} autoPlay={false} infiniteLoop>\n      {habits[0] ? (\n        habits.map((habit) => <Card habit={habit} key={habit.id} />)\n      ) : (\n        <CardDefault />\n      )}\n    </Container>\n  );\n};\n\nexport default CardList;\n","/home/gustazacchi/gestao-de-habitos/src/components/Button/index.jsx",[],["291","292"],"/home/gustazacchi/gestao-de-habitos/src/components/Input/index.jsx",[],"/home/gustazacchi/gestao-de-habitos/src/components/CardGroupList/index.jsx",["293","294","295"],"import { Container } from \"./styles\";\nimport CardGroup from \"../CardGroup\";\nimport { useContext } from \"react\";\nimport { useEffect } from \"react\";\nimport { useMyGroups } from \"../../providers/MyGroups\";\nimport CardGroupDefault from \"../CardGroupDefault\";\nimport { useState } from \"react\";\nconst CardGroupList = () => {\n  const { myGroups, loadGroups } = useMyGroups();\n\n  useEffect(() => {\n    loadGroups();\n    console.log(myGroups);\n  }, []);\n\n  return (\n    <Container showThumbs={false} autoPlay={false}>\n      {myGroups[0] ? (\n        myGroups.map((group) => <CardGroup key={group.id} group={group} />)\n      ) : (\n        <CardGroupDefault />\n      )}\n    </Container>\n  );\n};\n\nexport default CardGroupList;\n","/home/gustazacchi/gestao-de-habitos/src/components/NewGroup/index.jsx",[],"/home/gustazacchi/gestao-de-habitos/src/components/CardDiscovery/index.jsx",[],"/home/gustazacchi/gestao-de-habitos/src/components/Habits/NewHabit/index.jsx",["296"],"import api from \"../../../services/api\";\nimport * as yup from \"yup\";\nimport { useForm } from \"react-hook-form\";\nimport { yupResolver } from \"@hookform/resolvers/yup\";\nimport { makeStyles } from \"@material-ui/core/styles\";\nimport { Button, MySelect, Container, Input } from \"./styles\";\nimport { toast } from \"react-toastify\";\nimport { useContext } from \"react\";\nimport { HabitsContext, useHabits } from \"../../../providers/habits\";\n\nconst NewHabit = ({ handleClose }) => {\n  const formSchema = yup.object().shape({\n    title: yup.string().min(2, \"Mininum 2 characters\").required(\"Required\"),\n    category: yup.string().min(2, \"Mininum 2 characters\").required(\"Required\"),\n    difficulty: yup.string().ensure().required(\"Required\"),\n    frequency: yup.string().ensure().required(\"Required\"),\n  });\n\n  const {\n    register,\n    handleSubmit,\n    formState: { errors },\n  } = useForm({\n    resolver: yupResolver(formSchema),\n  });\n  const { token, user_id, getHabits } = useContext(HabitsContext);\n  const onSubmitFunc = (data_user) => {\n    const data = {\n      category: data_user.category,\n      title: data_user.title,\n      user: user_id,\n      difficulty: data_user.difficulty,\n      frequency: data_user.frequency,\n      how_much_achieved: 0,\n    };\n    api.defaults.headers.authorization = `Bearer ${token}`;\n    api\n      .post(\"/habits/\", data)\n      .then((res) => {\n        toast.success(\"Habit created\");\n        getHabits();\n      })\n      .catch((err) => toast.error(\"Error in creating the habit\"));\n\n    handleClose();\n  };\n\n  const useStyles = makeStyles((theme) => ({\n    paper: {\n      backgroundColor: theme.palette.background.paper,\n      border: \"2px solid #000\",\n      boxShadow: theme.shadows[5],\n      margin: \"0 auto\",\n      display: \"flex\",\n      width: \"90%\",\n      borderRadius: \"4px\",\n      padding: \"0\",\n    },\n  }));\n  const classes = useStyles();\n\n  return (\n    <div className={classes.paper}>\n      <Container>\n        <div className=\"header\">\n          <strong>New Habit</strong>\n          <div onClick={() => handleClose()}>X</div>\n        </div>\n        <form onSubmit={handleSubmit(onSubmitFunc)}>\n          <Input\n            name=\"title\"\n            placeholder=\"Title\"\n            required\n            margin=\"normal\"\n            variant=\"outlined\"\n            label=\"Title\"\n            size=\"small\"\n            color=\"primary\"\n            {...register(\"title\")}\n            error={!!errors.title}\n            helperText={errors.title?.message}\n          />\n          <Input\n            name=\"category\"\n            placeholder=\"Category\"\n            required\n            margin=\"normal\"\n            variant=\"outlined\"\n            label=\"Category\"\n            size=\"small\"\n            color=\"primary\"\n            {...register(\"category\")}\n            error={!!errors.category}\n            helperText={errors.category?.message}\n          />\n\n          <MySelect name=\"difficulty\" {...register(\"difficulty\")}>\n            <option value=\"\" disabled selected>\n              Difficulty *\n            </option>\n            <option value=\"easy\">Easy</option>\n            <option value=\"moderated\">Moderated</option>\n            <option value=\"hard\">Hard</option>\n          </MySelect>\n\n          <MySelect name=\"frequency\" {...register(\"frequency\")}>\n            <option value=\"\" disabled selected>\n              Frequency *\n            </option>\n            <option value=\"three in three hours\">Three in three hours</option>\n            <option value=\"daily\">Daily</option>\n            <option value=\"weekly\">Weekly</option>\n          </MySelect>\n\n          <Button type=\"submit\">Register</Button>\n        </form>\n      </Container>\n    </div>\n  );\n};\nexport default NewHabit;\n","/home/gustazacchi/gestao-de-habitos/src/components/CardDiscovery/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/NewGroup/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/CardJoinGroup/index.jsx",[],"/home/gustazacchi/gestao-de-habitos/src/components/CardJoinGroup/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/Habits/NewHabit/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/Input/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/CardGroupList/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/Button/styles.js",[],"/home/gustazacchi/gestao-de-habitos/src/components/Habits/